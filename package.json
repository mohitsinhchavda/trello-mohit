    "lint": "eslint src config",
    "start": "webpack-cli serve --mode development --config config/webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohitsinhchavda/trello-mohit.git"
  },
