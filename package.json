  "bugs": {
    "url": "https://github.com/mohitsinhchavda/trello-mohit/issues"
  },
  "homepage": "https://github.com/mohitsinhchavda/trello-mohit#readme",
  "dependencies": {
    "axios": "^0.21.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "redux": "^4.1.0",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@svgr/webpack": "^5.5.0",
